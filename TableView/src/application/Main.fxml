            <JFXButton fx:id="btnMinimize" alignment="CENTER" contentDisplay="CENTER" layoutX="181.0" onAction="#minimizarVentana" text=" " AnchorPane.rightAnchor="73.0" AnchorPane.topAnchor="0.0">
