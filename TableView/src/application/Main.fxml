      <Pane fx:id="right" onMouseDragged="#resizeRightDragged" onMousePressed="#resizeHorizontalPressed" prefHeight="400.0" prefWidth="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
